# patronts SDK Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Security Configuration
# The SDK supports loading security credentials from environment variables
# with the prefix: PATRONTS_
#
# Security environment variables:
PATRONTS_BEARER_AUTH=your_bearerauth_here
PATRONTS_COOKIE_AUTH=your_cookieauth_here

# Debug Configuration
# Enable HTTP request/response logging for debugging
PATRONTS_DEBUG=true
